# 【开发】环境变量

# 应用部署基础路径（如部署在子目录 /admin，则设置为 /admin/）
VITE_BASE_URL = /

# API 请求基础路径（包含版本前缀）
VITE_API_URL = /api/v1

# 代理目标地址（开发环境通过 Vite 代理转发请求到此地址，解决跨域问题）
# 指向本地 Egg.js 接口服务（见 artd-egg-server，默认端口 7001）
VITE_API_PROXY_URL = http://localhost:7011

# 权限模式：使用后端接口返回的菜单与权限
VITE_ACCESS_MODE = backend

# Delete console
VITE_DROP_CONSOLE = false
